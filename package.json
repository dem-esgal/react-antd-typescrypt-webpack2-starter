{
  "name": "WP2-React_ant-Starter",
  "version": "0.0.1",
  "description": "WP2-React_ant-Starter",
  "scripts": {
    "build": "webpack --progress --colors",
    "build:prod": "NODE_ENV=production webpack -p",
    "build-win:prod": "set NODE_ENV=production&& webpack -p",
    "lint": "eslint src",
    "ts-compile": "tsc -p tsconfig.json",
    "tslint": "tslint --project ./tsconfig.json --config ./tslint.json --type-check ./src/**/*.ts ./src/**/*.tsx",
    "start": "webpack-dev-server --progress --colors",
    "test": "yarn ts-compile&&jest --no-cache"
  },
  "dependencies": {
    "antd": "2.10.2",
    "axios": "^0.16.1",
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "history": "^4.6.1",
    "js-cookie": "^2.1.3",
    "lodash": "^4.17.4",
    "moment": "^2.15.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.0.3",
    "react-motion": "^0.4.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-sortable-hoc": "^0.6.3",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.0",
    "@types/jest": "^19.2.3",
    "@types/node": "^7.0.22",
    "@types/react": "15.0.24",
    "@types/react-dom": "15.5.0",
    "@types/react-redux": "^4.4.35",
    "@types/react-router": "4.0.8",
    "@types/react-router-dom": "4.0.4",
    "@types/redux-thunk": "^2.1.31",
    "awesome-typescript-loader": "~3.1.2",
    "babel-core": "^6.23.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-amd-to-commonjs": "^0.2.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "20.0.4",
    "jsdom": "^7.2.2",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "node-sass": "^3.3.3",
    "postcss-loader": "^1.3.3",
    "react-test-renderer": "^15.3.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.16.1",
    "ts-jest": "^20.0.5",
    "tslint": "^5.4.2",
    "tslint-loader": "~3.4.3",
    "typescript": "~2.3.3",
    "uglify-js": "^2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4"
  },
  "jest": {
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(\\.(test))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js"
  }
}
